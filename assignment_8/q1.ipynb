    "def factorial(num):\n",
    "    if num>=0:\n",
    "        fact=1\n",
    "        for i in range (num, 1, -1):\n",
    "            fact=fact*i\n",
    "        print(fact)\n",
    "    else:\n",
    "        print(\"enter valid number!!\")\n",
    "fact = int(input(\"enter number : \"))\n",
    "factorial(fact)"
   ]
